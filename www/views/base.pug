doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        meta(name='viewport' content='width=device-width, initial-scale=1')
        title= YAWA
        link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous")
        link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css')
        link(rel='stylesheet' href='/css/sf.css?v=' + server_started)
        link#favicon(rel='shortcut icon' href='/favicon.ico')

    body
        div.wrapper.container-fluid
            div#content
                nav#toptoptop.navbar.navbar-expand-lg.m-0.p-0
                    a#yawaheader.navbar-brand.override(href='/')
                        img#headericon.d-inline-block(src='/img/icons/day_clear.svg' alt='Refresh: Yet Another Weather App' height='24px')
                        div.d-none.d-md-inline-block.text-dark &nbsp;YAWA
                    ul.navbar-nav.mr-auto
                    ul.navbar-nav
                        li#currentTime.d-none.d-md-block
                    ul.navbar-nav.mr-auto
                    ul.navbar-nav
                        li
                            span#location-clicker
                                i#geolocation.bi.bi-geo-alt.btn.btn-primary.btn-small 
                            input#location(readonly=false)
            block content
div#noactivity.collapse
    i.bi.bi-slash-circle
    i.bi.bi-filter
    i No activity found with current filter(s).
div.collapse
    div#spinner
        div.lds-ring
            div
            div
            div
            div
div#loading-zone
script
    | var start_time = Date.now()
